--made by Semyon

local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "MegMenu",
   Icon = 0, 
   LoadingTitle = "MegMenu",
   LoadingSubtitle = "Made by Semyon",
   Theme = "Default", 

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false, 

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, 
      FileName = "Big Hub"
   },

   Discord = {
      Enabled = false, 
      Invite = "noinvitelink", 
      RememberJoins = true 
   },

   KeySystem = false, 
   KeySettings = {
      Title = "Untitled",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided", 
      FileName = "Key", 
      SaveKey = true, 
      GrabKeyFromSite = false, 
      Key = {"Hello"} 
   }
})

local PlayerTab = Window:CreateTab("Skins", "shirt") 

local Button = PlayerTab:CreateButton({
   Name = "BlackSuit",
   Callback = function()
   game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("BlackSuitEvent"):FireServer()

 end,
})

